{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "object-curly-newline": ["error", { "ObjectPattern": { "multiline": true } }],
      "react/prop-types": ["off"],
      "react/jsx-filename-extension": ["off"],
      "react/react-in-jsx-scope": "off",
      "semi": "off"
    }
}
